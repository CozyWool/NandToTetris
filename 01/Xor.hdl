/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    //write your code here
    Not(in=a, out=notA);
    Nand(a=notA, b=b, out=notANandB);
    Nand(a=A, b=notb, out=aNandNotB);
    Nand(a=notANandB, b=notANandB, out=out);
}